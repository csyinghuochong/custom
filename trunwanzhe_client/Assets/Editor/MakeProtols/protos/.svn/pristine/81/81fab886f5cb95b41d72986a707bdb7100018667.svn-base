// 一些通用的数据
package pb_common;

message pbResult {
        optional int32 result = 1;                      // 返回结果
}

message pbId32 {
        optional int32 id = 1;                          // 32位的id信息
}

message pbId32R {
        repeated int32 id = 1;                          // 32位的[id1,id2,...]信息
}

message pbId64 {
        optional int64 id = 1;                          // 64位的id信息
}

message pbId64R {
        repeated int64 ids = 1;                         // 64位的id信息集合
}

message pbIdString {
        optional string id = 1;                         // 字符串ID
}

